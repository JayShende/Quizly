{
  "name": "quizly",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "start": "turbo run start --filter=web --filter=backend ",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test":"cd apps/backend && pnpm test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@10.18.0",
  "engines": {
    "node": ">=18"
  }
}
